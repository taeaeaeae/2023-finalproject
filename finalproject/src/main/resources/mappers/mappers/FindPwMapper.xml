<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="org.zerock.myapp.persistence.FindPwDAO">

    <select id="findPw" resultType="org.zerock.myapp.domain.FindPwVO">
        SELECT *
        FROM users 
        WHERE uids = #{uids} 
        AND email = #{email}
    </select>

  </mapper>
  
 
