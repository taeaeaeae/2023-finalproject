<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="org.zerock.myapp.persistence.FindIdDAO">

    <select id="findId" resultType="org.zerock.myapp.domain.FindIdVO">
        SELECT *
        FROM users 
        WHERE name = #{name} 
        AND phonenumber = #{phonenumber}
    </select>

  </mapper>
  
 
